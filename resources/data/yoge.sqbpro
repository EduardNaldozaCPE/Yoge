score.sessionId, 
score.step, 
pose.wght, 
pose.duration, 
score.leftElbow, 
score.rightElbow, 
score.leftKnee, 
score.rightKnee, 
score.leftShoulder, 
score.rightShoulder, 
score.leftHip, 
score.rightHip
FROM score 
INNER JOIN session ON score.sessionId = session.sessionId
INNER JOIN pose ON session.sequenceId = pose.sequenceId
WHERE scoreId=(SELECT MAX(scoreId) FROM score) AND score.step = pose.stepNum;</sql><sql name="SQL 3">-- SELECT poseId, sequenceId, stepNum, poseName, wght,   FROM pose
SELECT *  FROM pose
INNER JOIN score ON pose.stepNum = score.step AND score.step = pose.stepNum
INNER JOIN session ON score.sessionId = session.sessionId AND session.sequenceId = pose.sequenceId
WHERE session.sessionId = 1 AND score.step = 1
ORDER BY stepNum ASC; </sql><sql name="SQL 4">SELECT * FROM sequence
